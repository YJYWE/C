/********  分支语句  ********/

//if

#include <stdio.h>

void main()
{
    int n=0;
    for (int i=1; i<=100; i++) {
        if (i%2==1) {
            printf("%3d",i);
            n++;
        }
        
        if (n%5==0) {
            printf("\n");
        }
    }
}

//switch

#include <stdio.h>

void main()
{
    int day;
    scanf("%d",&day);
    switch (day) {
        case 1:
            printf("星期一\n");
            break;
        case 2:
            printf("星期2\n");
            break;
        case 3:
            printf("星期3\n");
            break;
        case 4:
            printf("星期4\n");
            break;
        case 5:
            printf("星期5\n");
            break;
        case 6:
            printf("星期6\n");
            break;
        case 7:
            printf("星期7\n");
            break;
        default:
            break;
    }
}

/********  循环语句  ********/

